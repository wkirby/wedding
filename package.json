{
  "name": "wedding",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "preexport": "next build",
    "export": "next export",
    "start": "next start",
    "s3:clean": "aws s3 rm s3://wyattandjessica --recursive --profile apsis",
    "s3:clean:staging": "aws s3 rm s3://wyattandjessica-staging --recursive --profile apsis",
    "s3:upload": "aws s3 sync out/ s3://wyattandjessica --acl public-read --profile apsis",
    "s3:upload:staging": "aws s3 sync out/ s3://wyattandjessica-staging --acl public-read --profile apsis",
    "cf:invalidate": "aws cloudfront create-invalidation --distribution-id E37UXYWN6RJZWD --paths \"/*\" --profile apsis",
    "predeploy": "npm run export",
    "predeploy:staging": "npm run export",
    "deploy": "npm run s3:clean && npm run s3:upload",
    "deploy:staging": "npm run s3:upload:staging",
    "postdeploy": "npm run cf:invalidate"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "bloomer": "^0.6.5",
