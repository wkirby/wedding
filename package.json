    "classnames": "^2.2.6",
    "iframe-resizer": "^4.1.1",
    "js-yaml-loader": "^1.0.1",
    "lodash": "^4.17.11",
    "modularscale-sass": "^3.0.8",
    "next": "^8.1.0",
    "next-page-transitions": "^1.0.0-beta.2",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-animated-burgers": "^1.2.6",
    "react-dom": "^16.8.6",
    "react-images": "^0.5.19",
    "react-portal": "^4.2.0",
    "react-safe": "^1.3.0",
    "react-skeleton-css": "^1.1.0",
    "react-stack-grid": "^0.7.1",
    "react-tooltip": "^3.10.0",
    "reactstrap": "^8.0.0",
    "smartquotes": "^2.3.1",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {}
}
